source leaprc.protein.ff14SB
source leaprc.water.opc 

ras = loadpdb ras.pdb
raf = loadpdb raf.pdb

com = combine{ras,raf}
set default PBRadii mbondi2

saveamberparm com ras-raf.prmtop ras-raf.rst7
saveamberparm ras ras.prmtop ras.rst7
saveamberparm raf raf.prmtop raf.rst7

solvatebox com TIP3PBOX 12.0

saveamberparm com ras-raf_solvated.prmtop ras-raf_solvated.rst7

quit

